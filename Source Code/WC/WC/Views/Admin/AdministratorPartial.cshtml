@using WC.Models
@model List<FriendViewModel>
    @foreach (var admin in Model)
{

    <div id="admin-block-@admin.FriendId" class="friend-request col-lg-3 col-md-4 col-sm-6 col-xs-12">

        <div class="msg-img">
            <img class="img-circle" src="@Url.Content(admin.ProfileImgUrl)" alt="@admin.Name" title="@admin.Name">

        </div>
        <p class="msg-name admin-name">
            <a href="@Url.Action(admin.UserName, "profile")">@admin.Name</a>
        </p>
        <i data-id="@admin.FriendId" class="fa-pull-right remove-admin glyphicon glyphicon-remove"></i>
    </div>
}
