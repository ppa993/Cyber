    <script>
        var fb_open = false;
        function InitChatFB() {
            if (!fb_open) {
                $("#sidebar-chat").removeClass("st-effect-1");
                $("#sidebar-chat").addClass("sidebar-closed");
                $("#sidebar-chat").css("display", "none");
            } else {
                $("#sidebar-chat").removeClass("sidebar-closed");
                $("#sidebar-chat").addClass("st-effect-1");
                $("#sidebar-chat").css("display", "block");
            }
        }
        $(document).ready(function () {
            $("#tunrsc").click(function () {
                fb_open = !fb_open; 
                InitChatFB();
            });
        });
    </script>
